#include <iostream>

using namespace std;

int main()
{
    int numbers [5];
    cout << "Enter 5 numbers: ";
    for (int bin = 0; bin < 5; bin++) {
        cin >> numbers[bin];
    }
    for (int bin = 0; bin < 5; bin++) {
        cout << numbers[bin];
        if (bin != 4) {
            cout <<", ";
        }
    }
    cout << endl;
}
